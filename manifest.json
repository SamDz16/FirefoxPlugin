{
	"manifest_version": 2,
	"name": "Trigger Louise Algorithms",
	"version": "1.0",
	"description": "Trigger Louise algorithms on the httsp://dbpedia.org/sparql whenever the user wants it",
	"content_scripts": [
		{
			"matches": ["https://dbpedia.org/sparql"],
			"js": [
				"libs/vuejs_lib.js",
				"libs/ajax.js",
				"wasm/assets/wasm_exec.js",
				"content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icons/bd.png",
		"default_popup": "popup.html"
	},
	"permissions": ["*://*/*"],
	"icons": {
		"16": "icons/bd.png",
		"48": "icons/bd.png",
		"128": "icons/bd.png"
	}
}
